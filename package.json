{
  "name": "arp",
  "version": "2.0.0",
  "description": "Adult Robbery Patterns",
  "license": "GPL-3.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jieidson/arp"
  },
  "scripts": {
    "ng": "ng",
    "prestart": "./scripts/version.sh",
    "start": "npm run build:simulator && ng serve",
    "build:simulator": "ng build simulator && cp dist/simulator/bundles/arp-simulator.umd.js client/assets/",
    "build:simulator:prod": "ng build --prod simulator && cp dist/simulator/bundles/arp-simulator.umd.js client/assets/",
    "prebuild:dev": "./scripts/version.sh",
    "build:dev": "npm run build:simulator && ng build client",
    "prebuild:prod": "./scripts/version.sh",
    "build:prod": "npm run build:simulator:prod && ng build client",
    "pretest": "./scripts/version.sh",
    "test": "ng test",
    "prelint": "./scripts/version.sh",
    "lint": "ng lint",
    "pree2e": "./scripts/version.sh",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "6.1.1",
    "@angular/cdk": "6.4.2",
    "@angular/common": "6.1.1",
    "@angular/compiler": "6.1.1",
    "@angular/core": "6.1.1",
    "@angular/forms": "6.1.1",
    "@angular/material": "6.4.2",
    "@angular/platform-browser": "6.1.1",
    "@angular/platform-browser-dynamic": "6.1.1",
    "@angular/pwa": "0.7.2",
    "@angular/router": "6.1.1",
    "@angular/service-worker": "6.1.1",
    "core-js": "2.5.7",
    "hammerjs": "2.0.8",
    "rxjs": "6.2.2",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.7.2",
    "@angular-devkit/build-ng-packagr": "0.7.2",
    "@angular/cli": "6.1.2",
    "@angular/compiler-cli": "6.1.1",
    "@angular/language-service": "6.1.1",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "10.5.7",
    "codelyzer": "4.4.3",
    "jasmine": "3.1.0",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.5",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.2.0",
    "ng-packagr": "3.0.6",
    "protractor": "5.4.0",
    "rxjs-tslint": "0.1.5",
    "ts-node": "7.0.0",
    "tsickle": "0.32.1",
    "tslib": "1.9.3",
    "tslint": "5.11.0",
    "tslint-angular": "1.1.2",
    "typescript": "2.9.2"
  }
}
